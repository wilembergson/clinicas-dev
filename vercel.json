{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "devIndicators": {
            "enabled": false
          }
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "status": 200,
        "headers": {
          "cache-control": "public, max-age=0",
          "strict-transport-security": "max-age=63072000; includeSubDomains; preload"
        },
        "continue": true
      }
    ]
  }